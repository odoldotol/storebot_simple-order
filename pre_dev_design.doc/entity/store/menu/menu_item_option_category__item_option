# menu_item_option_category__item_option

- PK(store_id, item_option_category_id, order_int, item_option_id)
- UNIQUE(item_option_category_id, item_option_id)


store_id(store.id) [fk]

item_option_category_id(item_option_category.id) [fk, delete: cascade]
item_option_id(item_option.id) [fk]

order_int(smallint)